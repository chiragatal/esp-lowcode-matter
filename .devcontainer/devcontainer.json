{
	"name": "esp-lowcode-matter-v0.6",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"GAEsp.vscode-lowcode-extension-beta"
			],
			"openFiles": [
				"README.md"
			]
		},
		"codespaces": {
			"extensions": [
				"GAEsp.vscode-lowcode-web-extension-beta"
			],
			"openFiles": [
				"README.md"
			]
		}
	},
	"workspaceFolder": "/workspaces/esp-lowcode-matter",
	"mounts": [
		"source=${localWorkspaceFolder},target=/home/espressif/esp-lowcode-matter,type=bind,consistency=cached",
		"source=${localWorkspaceFolder}/../esp-idf-release-v5.3,target=/home/espressif/esp-idf-release-v5.3,type=bind,consistency=cached",
		"source=${localWorkspaceFolder}/../esp-amp,target=/home/espressif/esp-amp,type=bind,consistency=cached"
	]
}
